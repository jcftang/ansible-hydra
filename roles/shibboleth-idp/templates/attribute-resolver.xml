    <!-- Example LDAP Connector -->
    <resolver:DataConnector id="myLDAP" xsi:type="dc:LDAPDirectory"
        ldapURL="ldap://{{ shibboleth_user_ldap_server }}" 
        baseDN="{{ shibboleth_user_base_DN }}" 
        principal="{{ shibboleth_user_ldap_principal }}"
        principalCredential="{{ shibboleth_user_ldap_principal_password }}">
        <dc:FilterTemplate>
            <![CDATA[
                (uid=$requestContext.principalName)
            ]]>
        </dc:FilterTemplate>

	
    </resolver:DataConnector>
    
